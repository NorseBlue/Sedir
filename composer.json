{
    "name": "norse-blue/seidr",
    "type": "library",
    "description": "Seidr is a framework agnostic package for easy XML data extraction.",
    "keywords": [
        "NorseBlue",
        "Seidr",
        "XML"
    ],
    "homepage": "https://github.com/NorseBlue/Seir",
    "license": "MIT",
    "authors": [
        {
            "name": "Axel Pardemann",
            "email": "axel.pardemann@norse.blue",
            "homepage": "https://norse.blue",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "norse-blue/sekkr": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "NorseBlue\\Seidr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NorseBlue\\Seidr\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
